#include <math.h>
int main() {
    float a;
    char b;
    int money=20;
    scanf("%f %c", &a, &b);
      if(a>0&&a<=1)
    {
        if(b=='y')
            printf("25");
        else
            printf("20");
    }
  if(a>1)
  {
      money=money+ceil(a-1);    //向上取整函数
      if(b=='y')
          printf("%d",money+5);
      else
          printf("%d",money);
  }

   
}
